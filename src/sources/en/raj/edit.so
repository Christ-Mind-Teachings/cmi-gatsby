"
" find <div markdown="1" class="well book"> and replace with {: .well.book}
" - do the same for class="well person"
"
" Used this command for batch processing
"
" $ for i in 2002 2003 2004 2005 2006 2007 2008 2009 2010 2011 2012 2013 2014 2015 2016 2017 2018; do
" $   cd sg${i}
" $   for k in *; do
" $     vi --standalone -es -S ../edit.so $k
" $   done
" $   cd ..
" $ done
"
let @b = '{: .well.book}'
let @c = '{: .well.person}'
let @q = 'dd/<\/div>"bPjdd'
let @r = 'dd/<\/div."cPjdd'

g/"well book"/normal @q
g/"well person"/normal @r

wq

